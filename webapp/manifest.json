{
  "_version": "1.40.0",
  "sap.app": {
    "id": "logaligroup.finalproject",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap-ux/fiori-freestyle-writer:basic",
      "version": "0.11.15",
      "toolsId": "3fd0f2b6-bd6f-45aa-afbf-f76e741b8790"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      },
      "ZEMPLOYEES_SRV": {
        "uri": "sap/opu/odata/sap/ZEMPLOYEES_SRV",
        "type": "OData",
        "settings": {
          "localUri": "localService/ZEMPLOYEES_SRV/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.102.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "logaligroup.finalproject.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      },
      "employeesModel": {
        "dataSource": "ZEMPLOYEES_SRV",
        "preload": true,
        "settings": {
            "defaultOperationMode":"Server",
            "defaultBindingMode": "TwoWay",
            "defaultCountMode": "Request",
            "refreshAfterChange": false,
            "useBatch": false,
            "defaultUpdateMethod": "Put"        
        }
      } 
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "logaligroup.finalproject.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "menu",
          "pattern": "",
          "target": [
            "menu"
          ]
        },
        {
            "name": "RouteCreate",
            "pattern": "create",
            "target": [
              "TergetCreate"
            ]
          }
      ],
      "targets": {
        "menu": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": true,
          "viewId": "Menu",
          "viewName": "Menu"
        },
        "TergetCreate": {
            "viewType": "XML",
            "transition": "slide",
            "clearControlAggregation": true,
            "viewId": "CreateEmployee",
            "viewName": "CreateEmployee"
          }
      }
    },
    "rootView": {
      "viewName": "logaligroup.finalproject.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
